{
  "manifest_version": 1,

  "name": "Let me work!",
  "description": "This extension allows you to block sites for a given time so you can focus on your work!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"

  ],

  "background" : {
    "scripts": []
  },
  "web_accessible_resources": ["blank.gif"]
}



